  "words": [
    { "info": "あんずの花", "info2": "あんずのはな", "word": "anzunohana" },
    { "info": "小川 未明", "info2": "おがわ みめい", "word": "ogawa mimei" },
    {
      "info": "私の家にきた盲目、",
      "info2": "わたしのうちにきためくら、",
      "word": "watasinoutinikitamekura,"
    },
    {
      "info": "帰りにあんずの花折って、",
      "info2": "かえりにあんずのはなおって、",
      "word": "kaerinianzunohanaotte,"
    },
    {
      "info": "夏がきたら、またこよう。",
      "info2": "なつがきたら、またこよう。",
      "word": "natugakitara,matakoyou."
    },
    {
      "info": "赤いお月さま出る晩に。",
      "info2": "あかいおつきさまでるばんに。",
      "word": "akaiotukisamaderubannni."
    },
    {
      "info": "俺はくるくる青坊主、",
      "info2": "おれはくるくるあおぼうず、",
      "word": "orehakurukuruaobouzu,"
    },
    {
      "info": "笠をかぶって坊ちゃんの、",
      "info2": "かさをかぶってぼっちゃんの、",
      "word": "kasawokabuttebottyannno,"
    },
    {
      "info": "お家の窓から、のぞきましょう。",
      "info2": "おいえのまどから、のぞきましょう。",
      "word": "oienomadokara,nozokimasyou."
    }
  ],
