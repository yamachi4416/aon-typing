{
  "private": true,
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "packageManager": "pnpm@10.13.1",
  "browserslist": [
    "since 2023-03-01 and not Firefox < 121"
  ],
  "scripts": {
    "postinstall": "nuxt prepare",
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "lint": "pnpm run '/lint:.*/'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint app/**/*.{scss,vue}",
    "test": "vitest",
    "test:unit": "vitest --project libs --project app",
    "test:e2e": "vitest --project e2e",
    "script": "tsx --tsconfig ./scripts/tsconfig.json",
    "script:preview:dist": "pnpm run script ./scripts/preview-server -d ./dist",
    "script:generate:problem": "pnpm run script ./scripts/problem generate -i ./data -o ./app/assets/api"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "citty": "^0.1.6",
    "eslint": "^9.33.0",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-promise": "^7.2.1",
    "happy-dom": "^18.0.1",
    "nuxt": "^4.0.3",
    "playwright-core": "^1.54.2",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-browser-compat": "1.0.0-beta.173",
    "stylelint-config-recess-order": "^7.2.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "terser": "^5.43.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "dependencies": {
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/seo": "^3.1.0",
    "vue-gtag-next": "^1.14.0"
  },
  "volta": {
    "node": "24.3.0",
    "pnpm": "10.13.1"
  }
}
