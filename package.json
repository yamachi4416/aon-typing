{
  "private": true,
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "packageManager": "pnpm@10.28.0",
  "browserslist": [
    "since 2023-03-01 and not Firefox < 121"
  ],
  "scripts": {
    "postinstall": "nuxt prepare",
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "lint": "pnpm run '/lint:.*/'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint app/**/*.{scss,vue}",
    "test": "vitest",
    "test:unit": "vitest --project libs --project app",
    "test:e2e": "vitest --project e2e",
    "script": "tsx --tsconfig ./scripts/tsconfig.json",
    "script:preview:dist": "pnpm run script ./scripts/preview-server -d ./dist",
    "script:generate:problem": "pnpm run script ./scripts/problem generate -i ./data -o ./app/assets/api"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.13.0",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "^25.1.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "citty": "^0.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-compat": "^6.1.0",
    "eslint-plugin-format": "^1.3.1",
    "eslint-plugin-promise": "^7.2.1",
    "google-fonts-helper": "^3.7.3",
    "happy-dom": "^20.4.0",
    "nuxt": "^4.3.0",
    "playwright-core": "^1.58.1",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "stylelint": "^17.1.0",
    "stylelint-browser-compat": "1.0.0-beta.180",
    "stylelint-config-recess-order": "^7.6.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^17.0.0",
    "terser": "^5.46.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-component-type-helpers": "^3.2.4",
    "vue-tsc": "^3.2.4"
  },
  "dependencies": {
    "@nuxtjs/seo": "^3.4.0",
    "vue-gtag-next": "^1.14.0"
  }
}
