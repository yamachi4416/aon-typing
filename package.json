{
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "postinstall": "nuxt prepare",
    "build": "nuxt generate",
    "build:scripts": "node scripts/build-scripts.mjs",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "preview:dist": "node ./scripts/dist/preview-server.js -d ./dist",
    "generate": "nuxt generate",
    "generate:problem": "node ./scripts/dist/problem.js generate -i ./data/ -o ./src/assets/api",
    "lint": "run-p 'lint:*'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint src/**/*.{scss,vue}",
    "lint:format": "prettier . -c",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/content-disposition": "^0.5.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.13.0",
    "@types/yargs": "^17.0.32",
    "content-disposition": "^0.5.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.2.0",
    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.11.2",
    "playwright-core": "^1.44.1",
    "prettier": "^3.2.5",
    "sass": "^1.77.4",
    "stylelint": "^16.6.1",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "vue-gtag-next": "^1.14.0"
  }
}
