{
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "postinstall": "nuxt prepare",
    "build": "nuxt generate",
    "build:scripts": "node scripts/build-scripts.mjs",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "preview:dist": "node ./scripts/dist/preview-server.js -d ./dist",
    "generate": "nuxt generate",
    "generate:problem": "node ./scripts/dist/problem.js generate -i ./data/ -o ./src/assets/api",
    "lint": "concurrently 'pnpm:lint:*'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint src/**/*.{scss,vue}",
    "lint:format": "prettier . -c",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.15.4",
    "@types/content-disposition": "^0.5.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.9",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.1.2",
    "content-disposition": "^0.5.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-promise": "^7.2.1",
    "jsdom": "^26.0.0",
    "nuxt": "^3.15.2",
    "playwright-core": "^1.49.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sass": "^1.83.4",
    "stylelint": "^16.13.2",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.3",
    "vue-tsc": "^2.2.0"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^0.7.5",
    "vue-gtag-next": "^1.14.0"
  }
}
