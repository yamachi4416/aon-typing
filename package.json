{
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run generate",
    "build:scripts": "node scripts/build-scripts.mjs",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "preview:dist": "node ./scripts/dist/preview-server.js -d ./dist",
    "generate": "nuxt generate",
    "generate:problem": "node ./scripts/dist/problem.js generate -i ./data/problems -o ./src/assets/api",
    "lint": "run-p 'lint:*'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint src/**/*.{scss,vue}",
    "lint:format": "prettier {src,libs,scripts,test}/**/*.{scss,vue,ts} -c",
    "test": "nuxt test"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.6.5",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/prettier": "^2.7.1",
    "@types/yargs": "^17.0.13",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.6.3",
    "playwright-core": "^1.36.2",
    "prettier": "^2.7.1",
    "sass": "^1.63.6",
    "stylelint": "^15.9.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.3"
  },
  "dependencies": {
    "vue-gtag-next": "^1.14.0"
  }
}
