{
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run generate",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "preview:dist": "node ./scripts/dist/preview-server.js -d ./dist",
    "generate": "nuxt generate",
    "generate:problem": "node ./scripts/dist/problem.js generate -i ./data/problems -o ./src/assets/api",
    "build:scripts": "node scripts/build-scripts.mjs",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint src/**/*.{scss,vue}",
    "format:check": "prettier {src,libs,scripts}/**/*.{scss,vue,ts} -c"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/prettier": "^2.7.1",
    "@types/yargs": "^17.0.13",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdom": "^21.1.0",
    "nuxt": "^3.1.2",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.8.4",
    "vue-tsc": "^1.0.9"
  },
  "dependencies": {
    "vue-gtag-next": "^1.14.0"
  }
}
