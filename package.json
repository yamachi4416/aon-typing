{
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "postinstall": "nuxt prepare",
    "build": "nuxt generate",
    "build:scripts": "node scripts/build-scripts.mjs",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "preview:dist": "node ./scripts/dist/preview-server.js -d ./dist",
    "generate": "nuxt generate",
    "generate:problem": "node ./scripts/dist/problem.js generate -i ./data/ -o ./src/assets/api",
    "lint": "concurrently 'pnpm:lint:*'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint src/**/*.{scss,vue}",
    "lint:format": "prettier . -c",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.14.4",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/content-disposition": "^0.5.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.1.0",
    "content-disposition": "^0.5.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^6.6.0",
    "jsdom": "^25.0.1",
    "nuxt": "^3.14.159",
    "playwright-core": "^1.48.2",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "dependencies": {
    "vue-gtag-next": "^1.14.0"
  }
}
