{
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "postinstall": "nuxt prepare",
    "build": "nuxt generate",
    "build:scripts": "node scripts/build-scripts.mjs",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "preview:dist": "node ./scripts/dist/preview-server.js -d ./dist",
    "generate": "nuxt generate",
    "generate:problem": "node ./scripts/dist/problem.js generate -i ./data/ -o ./app/assets/api",
    "lint": "concurrently 'pnpm:lint:*'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint app/**/*.{scss,vue}",
    "lint:format": "prettier . -c",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.19.1",
    "@types/content-disposition": "^0.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-promise": "^7.2.1",
    "nuxt": "^3.17.5",
    "playwright-core": "^1.53.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sass": "^1.89.2",
    "stylelint": "^16.21.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/seo": "^3.0.3",
    "happy-dom": "^18.0.1",
    "vue-gtag-next": "^1.14.0"
  }
}
