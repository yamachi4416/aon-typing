{
  "private": true,
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "postinstall": "nuxt prepare",
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "preview:dist": "tsx ./scripts/preview-server -d ./dist",
    "generate": "nuxt generate",
    "generate:problem": "tsx ./scripts/problem generate -i ./data/ -o ./app/assets/api",
    "lint": "concurrently 'pnpm:lint:*'",
    "lint:typecheck": "nuxt typecheck",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint app/**/*.{scss,vue}",
    "lint:format": "prettier . -c",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.5.0",
    "@nuxt/test-utils": "^3.19.2",
    "@types/content-disposition": "^0.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.10",
    "@types/yargs": "^17.0.33",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-promise": "^7.2.1",
    "happy-dom": "^18.0.1",
    "nuxt": "^3.17.6",
    "playwright-core": "^1.53.2",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sass": "^1.89.2",
    "stylelint": "^16.21.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1"
  },
  "dependencies": {
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/seo": "^3.1.0",
    "vue-gtag-next": "^1.14.0"
  },
  "volta": {
    "node": "24.3.0",
    "pnpm": "10.12.4"
  }
}
